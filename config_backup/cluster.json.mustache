{
  "name": {{cluster.name}},
  "description": {{cluster.description}},
  "vxnet": {{cluster.vxnet}},
  "upgrade_policy": [
    "appv-70gegwmp",
    "appv-dm3ltpxw"
  ],
  "nodes": [{
    "container": {
      "type": "kvm",
      "zone": "pek3a",
      "image": "img-535q372l"
    },
    "count": {{cluster.work_node.count}},
    "cpu": {{cluster.work_node.cpu}},
    "memory": {{cluster.work_node.memory}},
    "instance_class": {{cluster.work_node.instance_class}},
    "volume": {
      "size": {{cluster.work_node.volume_size}}
    },
    "server_id_upper_bound": 255,
    "services": {
      "init": {
        "cmd": ""
      },
      "start": {
        "cmd": ""
      },
      "stop": {
        "cmd": ""
      },
      "upgrade": {
        "cmd": ""
      }
    },
    "vertical_scaling_policy": "sequential"
  },{
    "container": {
      "type": "kvm",
      "zone": "pek3a",
      "image": "img-535q372l"
    },
    "count": {{cluster.browser_node.count}},
    "cpu": {{cluster.browser_node.cpu}},
    "memory": {{cluster.browser_node.memory}},
    "instance_class": {{cluster.browser_node.instance_class}},
    "volume": {
      "size": {{cluster.browser_node.volume_size}}
    },
    "server_id_upper_bound": 255,
    "services": {
      "init": {
        "cmd": ""
      },
      "start": {
        "cmd": ""
      },
      "stop": {
        "cmd": ""
      },
      "upgrade": {
        "cmd": ""
      }
    },
    "vertical_scaling_policy": "sequential"
  }],
  "endpoints": {
    "client": {
      "port": 8888,
      "protocol": "tcp"
    }
  },
  "health_check": {
    "enable": true,
    "interval_sec": 60,
    "timeout_sec": 10,
    "action_timeout_sec": 30,
    "healthy_threshold": 2,
    "unhealthy_threshold": 2,
    "check_cmd": "",
    "action_cmd": ""
  },
  "monitor": {
    "enable": true,
    "cmd": "",
    "items": {
      "mode": {
        "unit": "",
        "value_type": "str",
        "statistics_type": "latest",
        "enums": ["L", "F", "S"]
      },
      "min": {
        "unit": "ms",
        "value_type": "int",
        "statistics_type": "min",
        "scale_factor_when_display": 1
      },
      "avg": {
        "unit": "ms",
        "value_type": "int",
        "statistics_type": "avg",
        "scale_factor_when_display": 1
      },
      "max": {
        "unit": "ms",
        "value_type": "int",
        "statistics_type": "max",
        "scale_factor_when_display": 1
      },
      "received": {
        "unit": "count",
        "value_type": "int",
        "statistics_type": "latest",
        "scale_factor_when_display": 1
      },
      "sent": {
        "unit": "count",
        "value_type": "int",
        "statistics_type": "latest",
        "scale_factor_when_display": 1
      },
      "active": {
        "unit": "count",
        "value_type": "int",
        "statistics_type": "latest",
        "scale_factor_when_display": 1
      },
      "outstanding": {
        "unit": "count",
        "value_type": "int",
        "statistics_type": "latest",
        "scale_factor_when_display": 1
      }
    },
    "groups": {
      "latency": ["min", "avg", "max"],
      "throughput": ["received", "sent"],
      "connections": ["active", "outstanding"]
    },
    "display": ["mode", "latency", "throughput", "connections"],
    "alarm": ["avg"]
  },
  "advanced_actions": ["change_vxnet", "scale_horizontal"],
  "env": {
    "TEST_PARA": {{env.test_parameter}},
    "ADMIN_USER_NAME":{{env.admin_username}},
    "ADMIN_USER_PASSWORD":{{env.admin_password}},
    "ADMIN_IS_ENABLE":{{env.admin_enabled}}
  }
}